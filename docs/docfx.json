{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        { "src": "C:/Users/Luke/source/repos/ase-boose-assignment-l-wilkinson/BOOSEapp", "files": ["**/*.csproj"] },
        { "src": "C:/Users/Luke/source/repos/ase-boose-assignment-l-wilkinson/BOOSETests", "files": ["**/*.csproj"] }
      ],
      "dest": "api",
      "folderLayout": "ByProject"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "BOOSE docs",
      "_appTitle": "BOOSE docs",
      "_enableSearch": true,
      "pdf": false
    }
  }
}